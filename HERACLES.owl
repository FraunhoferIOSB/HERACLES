    <j.0:materialHasPurpose>
      <j.0:MaterialPurpose rdf:about="Binder">
        <j.0:instanceDisplayName>Binder</j.0:instanceDisplayName>
        <j.0:instanceUniqueName>Binder</j.0:instanceUniqueName>
        <rdfs:label>Binder</rdfs:label>
